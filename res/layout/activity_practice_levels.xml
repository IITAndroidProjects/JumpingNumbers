<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_practice"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".PracticeLevels">

    <TableLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true" >

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
	    <ImageButton
	        android:id="@+id/imagebutton1"
	        android:background="@android:color/transparent"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:onClick="level1"
	        android:layout_marginBottom="10dp"
	        android:src="@drawable/level1" />
	    
	<ImageView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:src="@drawable/trans" />
	    	
	    <ImageButton
	        android:id="@+id/imagebutton2"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:onClick="level2"
	        android:background="@android:color/transparent"
	        android:layout_marginBottom="10dp"
	        android:src="@drawable/level2" />
	</TableRow>
    <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        
   	    <ImageButton
	        android:id="@+id/imagebutton3"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:onClick="level3"
	        android:background="@android:color/transparent"
	        android:layout_marginBottom="10dp"
	        android:src="@drawable/level3" />
	<ImageView
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:src="@drawable/trans" />
	    <ImageButton
	        android:id="@+id/imagebutton4"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:background="@android:color/transparent"
	        android:layout_marginBottom="10dp"
	        android:onClick="level4"
	        android:src="@drawable/level4" />
</TableRow>
</TableLayout>
	    <ImageButton
	        android:id="@+id/imagebutton5"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:onClick="level5"
	        android:layout_below="@+id/relativeLayout1"
	        android:background="@android:color/transparent"
	        android:src="@drawable/level5"
	        android:layout_centerHorizontal="true" />

<!--     <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/relativeLayout1"
        android:layout_alignParentLeft="true"
        android:src="@drawable/ll2" />
-->
</RelativeLayout>